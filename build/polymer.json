{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "dist/**",
    "assets/**",
    "elements*.json",
    "node_modules/ace-builds/src-min/theme-monokai.js",
    "node_modules/ace-builds/src-min/mode-html.js",
    "node_modules/ace-builds/src-min/worker-html.js",
    "index.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "./",
      "browserCapabilities": [
        "es2015",
        "push",
        "serviceworker",
        "modules"
      ],
      "html": {
        "minify": false
      },
      "css": {
        "minify": false
      },
      "js": {
        "minify": false,
        "compile": false
      },
      "bundle": false,
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}